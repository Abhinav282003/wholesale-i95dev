<div class="quick-order-list-wrapper">
  
  {% render 'quick-order-list' %}
</div>

{% schema %}
{
  "name": "Quick Order Link",
  "target": "section",
  "settings": [
    {
      "type": "text",
      "id": "title",
      "label": "Title",
      "default": "Quick Order Form"
    },
    {
      "type": "text",
      "id": "link_label",
      "label": "Link Text",
      "default": "Quick Order"
    }
  ]
}
{% endschema %}

